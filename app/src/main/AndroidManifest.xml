<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.srish.pieboard">


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WelcomeActivity"
            android:label="@string/Activity_Welcome" />
        <activity android:name=".Instrnc"
            android:label="@string/Activity_Instrnc">
        </activity>
        <activity
            android:name=".SelectActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/Activity_Select" />
        <activity
            android:name=".ParaActivity"
            android:label="@string/title_activity_para"
            android:parentActivityName=".SelectActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.srish.pieboard.SelectActivity" />
        </activity>
        <activity android:name=".VRActivity"
            android:label="Select Point"/>
        <activity android:name=".IRActivity"
            android:label="Select Point"/>
        <activity
            android:name=".showResultActivity"
            android:label="Result"/>
    </application>

</manifest>